{
	"manifest_version": 2,
	"name": "GraspML",
	"description": "ML concepts, on your finger tips !",
	"version": "1",
	"author": "Rishabh Misra",
	"author": "Jigyasa Grover",
	"browser_action": {
		"default_icon": "tab-icon.png",
		"default_title": "GraspML"
	},
	"content_scripts": [{
     "js": ["site_data.js"],
     "css": ["style.css"],
     "matches": ["https://ajax.googleapis.com/*", "https://cdnjs.cloudflare.com/*"]
   	}],
   	"content_security_policy": "script-src 'self' https://ajax.googleapis.com https://cdnjs.cloudflare.com; object-src 'self'",
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"permissions": ["activeTab"]
}
